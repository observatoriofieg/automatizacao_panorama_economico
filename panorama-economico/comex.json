                "render": [
                    {
                        "code": "function(el,x){\n     var dados = x.data;\n     var viz = this.viz;\n     \n     var dados_total = dados.filter(function(d){\n                  return d.PERIODO === \"Total\";\n                })\n     \n     var dados_anual = dados.filter(function(d){\n                  return d.PERIODO === \"Anual\";\n                })\n     \n     var dados_periodo = dados.filter(function(d){\n                  return d.PERIODO === \"Jan-Dez\";\n                })\n     \n     viz.data(dados_total).timeline(false)\n     \n     \n    viz.id({\"mute\": [\"SALDO\", \"CORRENTE\"]})\n    \n    viz.ui({\"value\":[\n      {\n        \"method\": function(value){\n          if (value == \"CO_ANO\") {\n            viz.data(dados_anual)\n              .time({\"value\": value,\n                     \"solo\": []})\n              .timeline(false)\n              .x(value)\n              .y(\"VL_FOB\")\n              .draw();\n          } else if (value == \"PERIODO2\") {\n           viz.data(dados_total)\n              .time({\"value\": value,\n                     \"solo\": []})\n              .timeline(false)\n              .x(value)\n              .y(\"VL_FOB\")\n              .draw();\n        } else {\n          viz.data(dados_periodo)\n              .time(false)\n              .x(\"PERIODO2\")\n              .y(\"VL_FOB\")\n              .draw();\n        }\n        },\n        \"label\": \"Periodicidade:\",\n        \"type\": \"toggle\",\n        \"value\": [{\"Mensal\": \"PERIODO2\"}, {\"Anual\": \"CO_ANO\"}, {\"Acumulado\": \"x\"}]\n      },\n      {\n        \"method\": function(value){\n          if (value === \"1\") {\n            value = [\"EXP\", \"IMP\"];\n          } else if (value === \"3\") {\n            value = [\"SALDO\"];\n          } else if (value === \"2\") {\n            value = [\"CORRENTE\"]\n          }\n          viz\n          .id({\"solo\": value})\n          .draw();\n        },\n        \"label\": \"Tipo:\",\n        \"type\": \"toggle\",\n        \"value\": [{\"Exportação/Importação\": \"1\"}, {\"Corrente\" : \"2\"}, {\"Saldo\": \"3\"} ]\n      },\n      {\n        \"method\": function(value){\n          if (value === \"1\") {\n            viz.type(\"line\")\n               .timeline(false)\n               .size(false)\n               .draw();\n            \n          } else if (value === \"2\") {\n            viz.type(\"bar\")\n               .timeline(false)\n               .draw();\n          }\n        },\n        \"label\": \"Gráfico:\",\n        \"type\": \"toggle\",\n        \"value\": [{\"Linha\": \"1\"}, {\"Barras\": \"2\"}]\n      }\n      ]})\n}",
                        "data": null
                    }
                ]
